{
  "wrapper": "div.collection-wrapper",
  "sections": {
    "product-filtering": {
      "type": "main-collection-filtering",
      "settings": {
        "desktop_position": "topbar",
        "enable_filtering": true,
        "enable_sorting": true
      }
    },
    "main": {
      "type": "collection",
      "blocks": {
        "image": {
          "type": "image",
          "settings": {}
        },
        "vendor": {
          "type": "vendor",
          "settings": {
            "productType": false
          }
        },
        "title": {
          "type": "title",
          "settings": {}
        },
        "price": {
          "type": "price",
          "settings": {}
        },
        "in-your-cart": {
          "type": "in-your-cart",
          "settings": {}
        }
      },
      "block_order": [
        "image",
        "vendor",
        "title",
        "price",
        "in-your-cart"
      ],
      "settings": {
        "collectionProductsPerRow": 3,
        "collageLayoutEnabled": false
      }
    }
  },
  "order": [
    "product-filtering",
    "main"
  ]
}